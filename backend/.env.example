# Environment Configuration
# Copy this file to .env and fill in the values
# Example: cp .env.example .env

# General settings
ENV=development  # development or production
DEBUG=true  # Set to false in production

# API settings
API_BASE_URL=http://localhost:8000  # Base URL for API access

# Database settings
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/person_detection  # PostgreSQL connection string

# Storage directories
UPLOAD_DIR=uploads  # Directory for uploaded images
RESULTS_DIR=results  # Directory for detection results 